<h1>Just Five Songs</h1>

<table>
  <tr>
    <th>title</th>
    <th>album</th>
    <th>artist</th>
    <th>watch</th>
    <th>edit</th>
  </tr>
  <% @songs.each do |song| %>
    <tr>
      <td><%= song.title %></td>
      <td><%= song.album %></td>
      <td><%= song.artist %></td>
      <td><%= link_to "watch", song.youtube_link %></td>
      <td><%= link_to "edit", song_path(song.id) %></td>
    </tr>
  <% end %>
</table>
